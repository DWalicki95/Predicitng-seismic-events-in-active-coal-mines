1)\n","    #slope of linear regression\n","    X = pd.DataFrame()\n","    X['Hour'] = np.arange(25-last_X_hours, 25)\n","    X = sm.add_constant(X, prepend=True, has_constant='add')\n","    slopes = []\n","    for idx, row in df.iterrows():\n","      Y = row[ts[-last_X_hours:]]\n","      model = sm.OLS(endog=Y, exog=X.values)\n","      fitted_model = model.fit()\n","      slopes.append(fitted_model.params[1])\n","    df['slope_of_lr'] = slopes\n","\n","    df.drop(df[ts].iloc[:, :-1], inplace=True, axis=1)\n","    \n","    return df"],"metadata":{"id":"WacX6d2oGLQq"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["#time series features basic statstics computing in train dataset\n","for ts in ts_to_agg:\n","  ts_compute_stats(data_train_X, ts, False, 5)"],"metadata":{"id":"0zZOai3BL2_B"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["#time series features basic statstics computing in test dataset\n","for ts in ts_to_agg:\n","  ts_compute_stats(data_test_X, ts, False, 5)"],"metadata":{"id":"vOu92YCx8qoN"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["#time series features statstics computing in train dataset,\n","# for features where ABS stats needed\n","for ts in ts_to_agg_diff:\n","  ts_compute_stats(data_train_X, ts, True, 5)"],"metadata":{"id":"TtPeE_NcYFp7"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["#time series features statstics computing in test dataset, \n","#for features where ABS stats needed\n","for ts in ts_to_agg_diff:\n","  ts_compute_stats(data_test_X, ts, True, 5)"],"metadata":{"id":"4HzWEHWy8sky"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["data_train_X.drop('main_working_id', axis=1, inplace=True)\n","data_test_X.drop('main_working_id', axis=1, inplace=True)"],"metadata":{"id":"xYFFSMLdWYMU"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["#storing variables\n","data_train_X.to_csv('/content/drive/MyDrive/Data Science 